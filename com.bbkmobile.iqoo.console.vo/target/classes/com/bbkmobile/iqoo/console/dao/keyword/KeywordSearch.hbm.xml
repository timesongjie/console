<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.bbkmobile.iqoo.console.dao.keyword.KeywordSearch" table="t_keyword_search">
        <id name="id" type="java.lang.Long">
            <column name="id" />
            <generator class="identity" />
        </id>
<!--        		
		<many-to-one name="keyword"
			class="com.bbkmobile.iqoo.console.dao.keyword.Keyword" column="keyword_id"
			cascade="search" outer-join="true" fetch="select" lazy="false" not-null="true" />
--> 
		<many-to-one name="keyword"
			class="com.bbkmobile.iqoo.console.dao.keyword.Keyword" column="keyword_id"
			outer-join="true" fetch="select" lazy="false" not-null="true" />
			 		
		<property name="search_date" type="timestamp">
			<column name="search_date" length="19" not-null="true">
			</column>
		</property>   
    </class>

</hibernate-mapping>