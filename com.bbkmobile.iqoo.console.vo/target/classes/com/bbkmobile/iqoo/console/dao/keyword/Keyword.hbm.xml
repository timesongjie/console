<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.bbkmobile.iqoo.console.dao.keyword.Keyword" table="t_keyword_info">
        <id name="id" type="java.lang.Long">
            <column name="id" />
            <generator class="identity" />
        </id>
        
        <property name="keyword" type="string">
            <column name="keyword" length="40" not-null="true" >
            </column>
        </property>
        
        <property name="sequence" type="java.lang.Integer">
            <column name="sequence" length="11" not-null="true">
            </column>
        </property>

  		<property name="recommend" type="java.lang.Character">
            <column name="recommend" length="1" not-null="true">
            </column>
        </property>
        
        <property name="rate_rank" type="java.lang.Integer">
            <column name="rate_rank" length="11" not-null="true">
            </column>
        </property>

        <property name="add_date" type="timestamp">
            <column name="add_date" length="19" not-null="true">
            </column>
        </property>
        
        <property name="modify_date" type="timestamp">
            <column name="modify_date" length="19">
            </column>
        </property>
    
    </class>
</hibernate-mapping>