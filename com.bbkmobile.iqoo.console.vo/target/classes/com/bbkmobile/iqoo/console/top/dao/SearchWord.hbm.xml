<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.bbkmobile.iqoo.console.top.dao.SearchWord" table="t_search_hot_word" >
        <id name="id" type="java.lang.Long">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="word" type="string">
            <column name="word"  >
            </column>
        </property>
        
        <property name="search_count" type="java.lang.Long">
            <column name="search_count" >
            </column>
        </property>
        
        <property name="show_order" type="java.lang.Integer">
            <column name="show_order" >
            </column>
        </property>
		                 
        <property name="add_date" type="timestamp">
            <column name="add_date" length="19" >
            </column>
        </property>
    </class>
</hibernate-mapping>