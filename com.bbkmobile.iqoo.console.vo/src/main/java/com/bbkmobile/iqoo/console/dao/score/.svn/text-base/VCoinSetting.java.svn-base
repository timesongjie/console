package com.bbkmobile.iqoo.console.dao.score;

import java.util.Date;

public class VCoinSetting {

    private Long id;
    private int base_point;              //出发点1：基于用户使用活跃度 2：基于内容创建活跃度 3：基于活动奖励
    private int specific_indicator;      //specific_indicator具体指标1：首次开通 2：使用时长 
                                         //3：使用频次（一周内连续登录几次） 4：多次使用/下载
                                         //5：创建内容数量（评论数量） 6：创建内容质量（评论是否有用） 7：其他
    private int indicator_paramenter;    //指标参数
    private int score;                   //数值
    private String user;                 //用户范围
    private Date modify_date;
    
    public Long getId() {
        return this.id;
    }
    public void setId(Long id) {
        this.id = id;
    }
    public int getBase_point() {
        return this.base_point;
    }
    public void setBase_point(int base_point) {
        this.base_point = base_point;
    }
    public int getSpecific_indicator() {
        return this.specific_indicator;
    }
    public void setSpecific_indicator(int specific_indicator) {
        this.specific_indicator = specific_indicator;
    }
    public int getIndicator_paramenter() {
        return this.indicator_paramenter;
    }
    public void setIndicator_paramenter(int indicator_paramenter) {
        this.indicator_paramenter = indicator_paramenter;
    }
    public int getScore() {
        return this.score;
    }
    public void setScore(int score) {
        this.score = score;
    }
    public String getUser() {
        return this.user;
    }
    public void setUser(String user) {
        this.user = user;
    }
    public Date getModify_date() {
        return this.modify_date;
    }
    public void setModify_date(Date modify_date) {
        this.modify_date = modify_date;
    }
}
